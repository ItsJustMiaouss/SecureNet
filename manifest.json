{
  "name": "SecureNet",
  "description": "SecureNet",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["securenet.js"]
    }
  ],
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["view/warning.html"],
      "matches": ["<all_urls>"]
    }
  ]
}